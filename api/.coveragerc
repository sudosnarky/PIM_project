# .coveragerc - Coverage configuration
[run]
source = .
omit = 
    */venv/*
    */tests/*
    */__pycache__/*
    */node_modules/*
    main_legacy_backup.py
    setup.py
    conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

show_missing = True
skip_covered = False

[html]
directory = htmlcov
title = PARA InfoSystem Coverage Report

[xml]
output = coverage.xml